<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="2" parallel="tests" name="Suite">
	<test name="Verify Registration-Chrome">
		<parameter name="os" value="windows"></parameter>
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="testcases.TC001_AccountRegistration" />
		</classes>
	</test>
	<test name="Verify Registration-Edge">
		<parameter name="os" value="windows"></parameter>
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="testcases.TC001_AccountRegistration" />
		</classes>
	</test>
	<test name="Verify Registration-FireFox">
		<parameter name="os" value="windows"></parameter>
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="testcases.TC001_AccountRegistration" />
		</classes>
	</test>
	<test name="Verify Login-Chrome">
		<parameter name="os" value="windows"></parameter>
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="testcases.TC002_Login" />
		</classes>
	</test>
	<test name="Verify Login-Edge">
		<parameter name="os" value="windows"></parameter>
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="testcases.TC002_Login" />
		</classes>
	</test>
	<test name="Verify Login-FireFox">
		<parameter name="os" value="windows"></parameter>
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="testcases.TC002_Login" />
		</classes>
	</test>
</suite>
